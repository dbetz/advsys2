var dummy;

object foo {
p1: 10;
}

property p2;

def getp(obj, prop)
{
    var value;
    try {
        value = obj.(prop);
    }
    catch (e) {
        value = 0;
    }
    return value;
}

def getc()
{
    asm {
        TRAP 0
    }
}

def main()
{
    var ch;
    print getp(foo, p1);
    print getp(foo, p2);
    while ((ch = getc()) != 10)
        print ch;
}
